{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "class-methods-use-this": "off"
  }
}
